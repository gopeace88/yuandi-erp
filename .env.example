# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_API_KEY=your-public-anon-key
SUPABASE_API_KEY=your-private-service-key

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:8081
NEXT_PUBLIC_APP_NAME=YUANDI Collection Management
NEXT_PUBLIC_APP_VERSION=1.0.0

# Sentry Error Tracking (Optional)
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
SENTRY_AUTH_TOKEN=your-sentry-auth-token
SENTRY_PROJECT=yuandi-erp
SENTRY_ORG=your-org

# Vercel Analytics (Auto-configured in Vercel)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# Environment
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Database Direct Connection (for migrations)
DATABASE_URL=postgresql://postgres:password@localhost:5432/yuandi

# CRON Secret (for scheduled jobs)
CRON_SECRET=your-cron-secret

# Session Secret
SESSION_SECRET=your-session-secret-at-least-32-characters-long
