{
  "name": "yuandi-collection-management",
  "version": "1.0.0",
  "description": "YUANDI Collection 주문/재고/배송 관리 시스템",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "build:optimize": "node scripts/optimize-build.js && next build",
    "install:ci": "npm install --legacy-peer-deps",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --testPathPattern=api",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api",
    "test:e2e": "playwright test",
    "test:e2e:scenarios": "playwright test e2e/scenarios",
    "test:e2e:performance": "playwright test e2e/performance",
    "test:e2e:security": "playwright test e2e/security",
    "test:e2e:all": "playwright test",
    "test:workflow:integrity": "jest --testPathPattern=business-workflow-integrity",
    "test:workflow:e2e": "playwright test e2e/scenarios/business-workflow-e2e.spec.ts",
    "test:workflow:all": "npm run test:workflow:integrity && npm run test:workflow:e2e",
    "test:large-scale:ui:integrity": "playwright test e2e/large-scale-ui-integrity.spec.ts",
    "test:large-scale:ui:inbound": "playwright test e2e/large-scale-ui-integrity.spec.ts --grep '상품입고'",
    "test:large-scale:ui:orders": "playwright test e2e/large-scale-ui-integrity.spec.ts --grep '주문생성'",
    "test:large-scale:ui:shipments": "playwright test e2e/large-scale-ui-integrity.spec.ts --grep '배송처리'",
    "test:large-scale:ui:delivery": "playwright test e2e/large-scale-ui-integrity.spec.ts --grep '배송완료'",
    "test:large-scale:ui:refunds": "playwright test e2e/large-scale-ui-integrity.spec.ts --grep '환불처리'",
    "test:large-scale:ui:full": "playwright test e2e/large-scale-ui-integrity.spec.ts --headed --slowMo=100",
    "test:system": "ts-node test/system/run-all-tests.ts",
    "test:system:reset": "ts-node test/system/01-database-reset.ts",
    "test:system:seed": "ts-node test/system/02-seed-products.ts && ts-node test/system/03-seed-orders.ts && ts-node test/system/04-seed-shipments.ts",
    "test:system:verify": "ts-node test/system/05-verify-integrity.ts",
    "test:system:functional": "ts-node test/system/06-functional-test.ts",
    "analyze": "node scripts/analyze-bundle.js",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "env:setup": "bash scripts/setup-vercel-env.sh",
    "env:pull": "vercel env pull .env.local",
    "optimize": "node scripts/optimize-build.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@vercel/analytics": "^1.1.0",
    "@vercel/speed-insights": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "next-themes": "^0.2.1",
    "pg": "^8.16.3",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.1.2",
    "resend": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^4.19.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "24.3.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "gzip-size": "^7.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.2.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2",
    "vercel": "^33.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nextjs",
    "supabase",
    "ecommerce",
    "order-management",
    "inventory-management",
    "typescript",
    "tailwindcss"
  ],
  "author": "YUANDI Collection Management <contact@yuandi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuandi/collection-management.git"
  },
  "main": "jest.config.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "bugs": {
    "url": "https://github.com/yuandi/collection-management/issues"
  },
  "homepage": "https://github.com/yuandi/collection-management#readme"
}